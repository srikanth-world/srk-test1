import xlrd

# Load the Excel file
workbook = xlrd.open_workbook('your_excel_file.xlsx')

# Create a dictionary to store column headers and their corresponding cell addresses with the specified interior color
col_header_map = {}

# Define the target interior color (RGB(172, 185, 130)) in its components
target_color = (172, 185, 130)

# Iterate through all the sheets in the workbook
for sheet in workbook.sheets():
    for row_index in range(sheet.nrows):
        for col_index in range(sheet.ncols):
            cell = sheet.cell(row_index, col_index)
            # Check if the cell's interior color matches the target color
            cell_xf = workbook.xf_list[cell.xf_index]
            interior_color = cell_xf.background.pattern_colour_index
            if interior_color == xlrd.BIFFRecords().colour_map[target_color]:
                col_header = sheet.cell_value(0, col_index)  # Assuming column headers are in the first row
                col_header_map[col_header] = (row_index + 1, col_index + 1)

# Print the column headers and their corresponding cell addresses with the specified interior color
for col_header, cell_address in col_header_map.items():
    print(f"Column Header: {col_header}, Cell Address: ({cell_address[0]}, {cell_address[1]})")
